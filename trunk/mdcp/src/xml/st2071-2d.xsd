        </xs:complexContent>
    </xs:complexType>
    
    <xs:element name="NotLocked" type="tns:NotLocked" substitutionGroup="types:Exception" />
    
    <xs:complexType name="Locked">
        <xs:complexContent>
            <xs:extension base="types:Exception">
