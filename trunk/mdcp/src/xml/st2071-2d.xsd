                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:element name="LockNotFound" type="tns:LockNotFound" substitutionGroup="types:Exception" />
    
    <xs:complexType name="TooManySessions">
