        </xs:complexContent>
    </xs:complexType>
    
    <xs:element name="NotAcquired" type="tns:NotAcquired" substitutionGroup="types:Exception" />
    
    <xs:complexType name="NotLocked">
        <xs:complexContent>
            <xs:extension base="types:Exception">
